//页面样式
.my-case
    min-width 947px
.red
    color red !important
i,em,strong
    font-style normal
.hilight
    color #ff9f00
    margin-right 3px
    margin-left 3px

div, ul, li, p, h2, textarea, span, label
    box-sizing border-box
    .blue
        color #1278c6 !important
        cursor pointer
    .power-button
        border 0
        outline 0
        padding 1px 6px 1px 6px
        align-items flex-start
        text-align center
        text-indent 0px
        text-shadow none
        display inline-block
        &.power-add  
            box-sizing border-box
            min-width 76px
            height 26px
            font-family "microsoft yahei", sans-serif
            font-size 14px
            letter-spacing 2px
            background-color #007aff
            color #ffffff
            &:hover
                cursor pointer
                background-color #1786ff
            &:before
                content ''
                display inline-block
                width 13px
                height 13px
                margin-right 9px
                position relative
                top 2px
                background url(../../assets/image/add_white.png) top right no-repeat
            
    .my-case
        font-size 14px
        .case-header
            padding 0
            margin-bottom 20px
            color #000
            h3
                font-size 24px
        .case-handle
            //padding-bottom 15px
            overflow hidden
            .create-case
                float left
                cursor pointer
                button
                    border none
                    color #39f
                    background-color #fff
                    outline none
                    vertical-align middle
                    cursor pointer
                    padding-left 0
                img
                    vertical-align middle
                    width 18px
                    height 18px
            .power-search
                float right
                position relative
                width 244px
                height 26px
                input
                    box-sizing border-box
                    position relative
                    display inline-block
                    vertical-align top
                    width 214px
                    height 28px
                    padding 4px 7px
                    cursor text
                    border 1px solid #d9d9d9
                    outline 0
                span
                    box-sizing border-box
                    position relative
                    display inline-block
                    vertical-align top
                    border 1px solid #d9d9d9
                    border-left 0
                    width 28px
                    height 28px
                    background #ffffff url('../../assets/image/search.png') no-repeat
                    cursor pointer
                    margin-left -3px
        .case-list
            //overflow auto
            min-height 400px
            table
                border-collapse collapse
                width 100%
                text-align center
                border-right 1px solid #e4e4e4
                border-bottom 1px solid #e4e4e4
                border-left 1px solid #e4e4e4
                border-top 1px solid #e4e4e4
                table-layout fixed
                .col1
                    color #aaa
                .col9
                    width 176px
                    overflow hidden
                    text-overflow ellipsis
                    white-space nowrap
                .col5
                    width 188px
                    text-align left
                    p
                        width 100%
                        height 20px
                        
                        .copy
                            width 20px
                            height 20px
                            display inline-block
                            vertical-align middle
                            cursor pointer
                            background url(../../assets/image/copy.png) no-repeat
                            display none
                        &:hover
                            .copy
                                display inline-block
                
                .col8
                    width 100px
                .col10
                    width 104px
                    text-align center
                .col11
                    width 136px

                button
                    border none
                    background-color #fff
                    cursor pointer
                .t-header
                    background-color #f5f5f5
                    color #333
                    th
                        box-sizing border-box
                        height 44px
                        border 1px solid #e4e4e4
                        text-align center
                        background #f5f5f5
                        font-size 14px
                        font-weight bold
                        padding 0
                        .power-filter-dropdown
                            display inline-block
                            position relative
                            z-index 10
                            cursor pointer
                            .filter-dorpdown-icon
                                display inline-block
                                width 24px
                                height 16px
                                vertical-align middle
                                background url(../../assets/image/filter-normal.png) no-repeat
                                background-position top center
                            ul
                                position absolute
                                top 20px
                                width 120px
                                border 1px solid #e4e4e4
                                &.fn-hide
                                    display none
                                &.fn-block-show
                                    display block
                                li
                                    height 30px
                                    line-height 30px
                                    text-align center
                                    background #ffffff
                                    padding-left 10px
                                    .power-checkbox
                                        display inline-block
                                        vertical-align middle
                                        width 16px
                                        height 16px
                                        input[type="checkbox"]
                                            display none
                                        label
                                            display inline-block
                                            width 16px
                                            height 16px
                                            background url(../../assets/image/normal.png) no-repeat 
                                            background-position center
                                            &:hover
                                                cursor  pointer
                                                background url(../../assets/image/normal-hover.png) no-repeat 
                                                background-position  center
                                    .checkbox-wraper
                                        line-height 0
                                        float left
                                        position relative
                                        top 7px
                                    .filter-name-wraper
                                        float left
                                        padding-left 10px

                tr .clo_1
                    color #39f
                    cursor pointer
                    
                td
                    box-sizing border-box
                    height 44px
                    border 1px solid #e4e4e4
                    text-align center
                    padding 10px 5px
                    color #000
                    word-wrap break-word;
                    .status 
                        &.s0 
                            color #4370bc
                        &.s1 
                            color #4cb43b
                        &.s2 
                            color #f46a6a
                    .more
                        padding-right 17px
                        background url(../../assets/image/list_more.png) no-repeat right center
                        color #a2a29f
                        cursor pointer
                    .reason
                        position relative
                        display inline-block
                        width 14px
                        height 14px
                        background url(../../assets/image/tip.png) no-repeat left center
                        cursor pointer
                        vertical-align middle
                        .err_msg{
                                display none
                                position: absolute;
                                top: -10px;
                                left: -50%;
                                z-index: 1060;
                                width: 200px;
                                padding: 9px 14px;
                                text-align: left;
                                background:#fff;
                                border: 1px solid #cccccc;
                                border: 1px solid rgba(0, 0, 0, 0.2);
                                border-radius: 6px;
                                -webkit-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
                                box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
                                white-space: normal;
                                transform: translate(-65%,-100%)
                                -ms-transform: translate(-65%,-100%)
                                -moz-transform: translate(-65%,-100%)
                                -webkit-transform: translate(-65%,-100%)
                                -o-transform: translate(-65%,-100%)
                                .arrow{
                                    position: absolute;
                                    display: block;
                                    width: 0;
                                    height: 0;
                                    border-color: transparent;
                                    border-style: solid;
                                    left: 66%;
                                    border-bottom-width: 0;
                                    border-top-color: #999999;
                                    border-top-color: rgba(0, 0, 0, 0.25);
                                    top: 100%;
                                    border-width: 11px;
                                }
                                .arrow:after{
                                    content: "";
                                    display: block;
                                    position: absolute; 
                                    bottom: -9px;
                                    margin-left: -10px;
                                    border-bottom-width: 0;
                                    border-width: 10px;
                                    width: 0;
                                    height: 0;
                                    border-color: transparent;
                                    border-top-color: #ffffff;
                                    border-style: solid;
                                }
                        }
                    .reason:hover .err_msg{
                        display block
                    }

        .updataBtn
            margin-top 15px
            input,button
                width 24px
                height 24px
            input
                width 26px
        .ex-pagination-contain
            float right
            margin-top 20px
        
    .power-rowselect
        display  inline-block 
        vertical-align  middle 
        position  relative 
        top  -1px 
        font-size  14px 
        line-height  1 
        &:hover
            cursor  pointer 
        
        ul
            position  absolute 
            top  100% 
            right  0 
            z-index  3 
            margin-top  -1px 
            border  1px solid #D1D2D3 
            background-color  #ffffff 
            min-width  100% 
            -webkit-box-sizing  border-box 
            box-sizing  border-box 
            width  96px 
            li
                position  relative 
                a
                    max-width  none 
                    -ms-text-overflow  initial 
                    text-overflow  initial 
                    height  27px 
                    line-height  27px 
                    position  relative 
                    display  block 
                    margin  0 0 
                    color  #454a51 
                    &:hover
                        background  #f2f2f2 

        .power-rowselect-link
            display  inline-block 
            border-width  1px 1px 0 
            border-style  solid 
            border-color  transparent 
            position  relative 
            z-index  2 
            padding  0 21px 0 5px 
            overflow  hidden 
            white-space  nowrap 
            -ms-text-overflow  ellipsis 
            text-overflow  ellipsis 
            vertical-align  bottom 
            height  22px 
            line-height  20px 
            color #1278c6
            &.disa
                color  #aaa 
            &.allow
                color  #1278c6 


        .power-rowselect-link-active
            border-color  #D1D2D3 
            background-color  #ffffff 
            color #1278c6

        .power-rowselect-icon
            position  absolute 
            top  4px 
            right  4px 
            width  16px 
            height  16px 
            z-index  2 
            background url(../../assets/image/more_gray.png) center center no-repeat
            background-position  center 
            &.active
                top  4px 
                background url(../../assets/image/more.png) center center no-repeat

        .power-rowselect-list-disabled
            background  #f2f2f2 
            &:hover
                cursor  not-allowed 
            a hover
                cursor  not-allowed 

.chart-header
  height 25px
  .chart-title
    font-size 18px
    float left
    margin-left 10px

.chart-body
  margin-top 10px
  height 45px
.bodybtn 
  -webkit-box-sizing border-box
  box-sizing border-box
  min-width 76px
  height 26px
  font-family "microsoft yahei", sans-serif
  font-size 14px
  letter-spacing 2px
  background-color #ffffff
  color #000000
  border 1px solid #dddddd
  margin-right 6px
  float left
  &:hover 
    cursor pointer
    border 1px solid #007aff
  &:focus
    outline:none
  &.active
    border 1px solid #007aff
  &.search
    background-color rgb(0, 122, 255) 
    color rgb(255, 255, 255)
.bodydate-div
  -webkit-box-sizing border-box
  box-sizing border-box
  font-family "microsoft yahei", sans-serif
  font-size 14px
  letter-spacing 2px
  background-color #ffffff
  margin-right 6px
  float left
.bodydate-date
  -webkit-box-sizing border-box
  box-sizing border-box
  width 141px
  font-family "microsoft yahei", sans-serif
  font-size 13px
  letter-spacing 1px
  background-color #ffffff
  color #000000
  border 1px solid #dddddd
  padding 3px
  margin 0 3px
  pointer cursor